  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "build-production": "npx cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "predeploy": "yarn run build-production",
