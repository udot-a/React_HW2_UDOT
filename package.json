  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "precise-commits": "^1.0.2",
    "pretty-quick": "^2.0.1"
